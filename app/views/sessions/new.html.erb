<h1>登录</h1>
 
<% if flash[:login_error] %>
  <p><%= flash[:login_error] %></p>
<% end %>
 
<%= form_for :session, url: :sessions_create do |f| %>
 
  <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </p>
 
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
 
  <p><%= f.button "登录" %></p>
<% end %>


<%= link_to '返回', controller: 'welcome'  , action: 'index'%>